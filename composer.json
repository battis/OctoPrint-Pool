{
    "name": "battis/octoprint-pool",
    "description": "Pooled services for a collection OctoPrint instances",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Seth Battis",
            "email": "seth@battis.net"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "vlucas/phpdotenv": "^3.4",
        "php-di/php-di": "^6.2.2",
        "slim/slim": "^4.5.0",
        "slim/psr7": "^1.2.0",
        "slim/http": "^1.0.0",
        "chadicus/slim-oauth2": "^3.1",
        "tuupola/cors-middleware": "^1.1",
        "monolog/monolog": "^2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env'); \""
        ]
    },
    "autoload": {
        "psr-4": {
            "Battis\\": "src/api/"
        }
    }
}
