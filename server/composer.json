{
    "name": "battis/octoprint-pool-server",
    "description": "Pooled services for a collection OctoPrint instances",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Seth Battis",
            "email": "seth@battis.net"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "battis/web-app-server": "*",
        "vlucas/phpdotenv": "^5.0",
        "php-di/php-di": "^6.2.2",
        "slim/slim": "^4.5.0",
        "slim/psr7": "^1.2.0",
        "slim/http": "^1.0.0",
        "chadicus/slim-oauth2": "^3.1",
        "tuupola/cors-middleware": "^1.1",
        "monolog/monolog": "^2.0",
        "ext-pdo": "*",
        "ext-json": "*",
        "battis/hydratable": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../web-app/server"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('../env/.env') || copy('../env/.env.example', '../.env'); \""
        ]
    },
    "autoload": {
        "psr-4": {
            "Battis\\OctoPrintPool\\": "src/"
        }
    }
}
